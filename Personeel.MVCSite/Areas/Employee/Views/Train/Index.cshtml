@model IEnumerable<Personeel.MVCSite.Models.TrainViewModels.AddTrainViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>培训首页</h2>

<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TrainSort)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TrainDes)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StartTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EndTime)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TrainSort)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TrainDes)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StartTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EndTime)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id=item.TrainGuid }) 

        </td>
    </tr>
}

</table>
